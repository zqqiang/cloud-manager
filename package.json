{
  "name": "cloud-manager",
  "version": "1.0.0",
  "description": "Cloud Manager",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-css": "lessc public/theme/project/less/main.less public/theme/project/css/main.css",
    "watch-js": "watchify public/main.js -d -o public/bundle.js -t [ babelify --presets [ es2015 react ] --plugins [transform-decorators-legacy] ] -v",
    "watch": "npm run watch-js | npm run watch-css",
    "build-css": "catw -c 'lessc -' 'public/theme/project/less/main.less' > public/theme/project/css/main.css",
    "build-js": "browserify public/main.js > public/bundle.js",
    "build": "npm run build-css && npm run build-js",
    "start": "supervisor index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zqqiang/cloud-manager.git"
  },
  "author": "zqqiang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zqqiang/cloud-manager/issues"
  },
  "homepage": "https://github.com/zqqiang/cloud-manager#readme",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "express": "^4.15.2",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "path": "^0.12.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "serve-favicon": "^2.4.2"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "less": "^2.7.2",
    "watchify": "^3.9.0"
  }
}
